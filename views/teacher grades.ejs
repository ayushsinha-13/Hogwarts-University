<main>
<h1>Grades</h1>

<section id="dataSubmit">



    <!-- Body -->

    <div class="container marks">
        <h1>Python</h1>
    </div>

    <div class="container marks-academics">
        <table class="table table-light table-hover">
            <thead class="thead-dark">
              <tr>
                <th scope="col" id="round-left">#</th>
                <th scope="col" id="middle">Name</th>
                <th scope="col" id="middle">Marks</th>
                <th scope="col" id=middle>Attendance</th>
                <th scope="col" id="middle">Grade</th>
                <th scope="col" id="round-right">Submit</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <%for(var i=0;i<student.length;i++){%>
                    <%var count = i+1;%>
                    <th scope="row"><%=count%></th>
                <td><%=student[i].name%></td>
                <form action="/cpp" method="post">
                    <td><input type="text" placeholder="Marks" id="marks" name="marks"><p><%=student[i].cpp[0].marks%></p></td>
                    <td><input type="text" placeholder="Attendance" id="attendance" name="attendance"><p><%=student[i].cpp[0].attendance%></p></td>
                    <td><input type="text" placeholder="Grade" id="grade" name="grade"><p><%=student[i].cpp[0].grade%></p></td>
                    <input type="hidden" value="<%=student[i].uid%>" name="uid">
                    <td><button type="submit" class="btn btn-dark" style="
                      width: 50px;
                  " ><i class="fas fa-check-circle"></i></button></td>
                </form>
              </tr>
                <%}%>
            </tbody>
          </table>
    </div>

